    "pretty-error": "4.0.0",
    "semver": "7.5.4",
    "yaml": "2.3.4"
  },
  "devDependencies": {
    "@types/eslint": "8.44.7",
    "@types/ejs": "3.1.5",
    "@types/inquirer": "9.0.6",
    "@types/node-fetch": "2.6.4",
    "cpy-cli": "5.0.0",
    "type-fest": "4.6.0"
