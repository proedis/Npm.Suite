import type { IconName } from '@fortawesome/fontawesome-common-types';

import type { ComposedSharedObjects } from '../interfaces/enums';


/* --------
 * Internal Types
 * -------- */
type SharedObjectsIcons = Partial<{
  [K in keyof ComposedSharedObjects]: Partial<%- '<Record<ComposedSharedObjects[K], IconName>>'%>
}>;


/* --------
 * Exports
 * -------- */
const sharedObjectsIcons: SharedObjectsIcons = {
<% sharedObjects.forEach(function (s) { %>
  <%- s.name %>: {
  <% s.values.forEach(function (value) { %>
    <%= value %>: 'question',<% }) %>
  },
<% }) %>
};

export default sharedObjectsIcons;
